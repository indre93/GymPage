<h1><%= @user.username %>'s GymPage</h1>

<div class="content">
   <%= render partial: "layouts/alerts", locals: { object: @user } %>

   <% @workouts.each do |workout| %>
      <fieldset>
         <legend><%= workout_date(workout) %></legend>
         <p>"<%= workout.description %>"</p>
         <p><%= workout.duration %></p><br>
         <p><%= link_to "See More Details", user_workout_path(workout.user, workout) %></p>
      </fieldset>
   <% end %>
</div>